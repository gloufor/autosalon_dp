<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".news_main">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="0sp"
        android:layout_marginEnd="0sp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="160dp"
            android:layout_marginTop="150dp"
            android:layout_marginEnd="180dp"
            android:text="@string/uslugi"
            android:id="@+id/uslugi"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="normal|bold"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="40dp"
            android:layout_marginTop="150dp"
            android:layout_marginEnd="270dp"
            android:id="@+id/contact"
            android:text="@string/contact"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="normal|bold"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="250dp"
            android:layout_marginTop="150dp"
            android:layout_marginEnd="40dp"
            android:text="@string/car"
            android:id="@+id/car"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="normal|bold"
            tools:ignore="TextContrastCheck" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="180dp"
            android:layout_marginBottom="0dp"
            android:elevation="20dp"
            android:orientation="vertical"
            android:paddingTop="30sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="-2dp"
            tools:layout_editor_absoluteY="75dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0sp">
                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="27sp"
                    android:textStyle="normal|bold"
                    android:textColor="#000000"
                    android:text="@string/news"
                    android:textSize="20sp" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10sp">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/textInputEditText4"
                    android:layout_width="370dp"
                    android:layout_height="100dp"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="22sp"
                    android:background="@xml/config_layout"
                    android:focusable="false"
                    android:gravity="top"
                    android:padding="7sp"
                    android:text="@string/text_one"
                    android:textColor="#757575"
                    android:textSize="19sp"
                    tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck,DuplicateClickableBoundsCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10sp">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/textInputEditText5"
                    android:layout_width="370dp"
                    android:layout_height="100dp"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="22sp"
                    android:background="@xml/config_layout"
                    android:focusable="false"
                    android:gravity="top"
                    android:padding="7sp"
                    android:text="@string/text_two"
                    android:textColor="#757575"
                    android:textSize="19sp"
                    tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck,DuplicateClickableBoundsCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10sp">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/textInputEditText1"
                    android:layout_width="370dp"
                    android:layout_height="100dp"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="22sp"
                    android:background="@xml/config_layout"
                    android:focusable="false"
                    android:gravity="top"
                    android:padding="7sp"
                    android:text="@string/text_three"
                    android:textColor="#757575"
                    android:textSize="19sp"
                    tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck,DuplicateClickableBoundsCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            </RelativeLayout>
        </LinearLayout>
        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="252dp"
            android:layout_height="141dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="130dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="50dp"
            app:srcCompat="@drawable/logotip" />

        <ImageView
            android:id="@+id/iv_back_button"
            android:layout_width="84dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="19dp"
            android:layout_marginEnd="355dp"
            android:src="@drawable/back_vector"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="-353dp"
            android:layout_marginTop="21dp"
            android:layout_toEndOf="@+id/iv_back_button"
            android:text="@string/logout"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="normal|bold" />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>