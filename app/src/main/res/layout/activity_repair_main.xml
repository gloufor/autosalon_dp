<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".accessories_main">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/iv_back_button13"
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:layout_marginTop="19dp"
            android:layout_marginStart="15dp"
            android:src="@drawable/back_vector"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/uslugi1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="8dp"
            android:layout_marginTop="15dp"
            android:layout_toEndOf="@+id/iv_back_button13"
            android:text="@string/repair"
            android:textColor="@color/black"
            android:textSize="25sp"
            android:textStyle="normal|bold"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="252dp"
            android:layout_height="141dp"
            android:layout_marginVertical="55dp"
            android:layout_centerHorizontal="true"
            app:srcCompat="@drawable/logotip" />

        <TextView
            android:id="@+id/texosmotropis"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="330dp"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            android:gravity="center"
            android:text="@string/opisrepairone"
            android:textSize="25sp"
            android:textColor="@color/black"/>
        <TextView
            android:id="@+id/opishepl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="480dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            android:text="@string/opisrepair"
            android:textSize="20sp"/>

        <TextView
            android:id="@+id/tozapis"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="200dp"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            android:gravity="center"
            android:textSize="25sp"
            android:textColor="@color/black"
            android:text="@string/repairzapis"/>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>